{
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": [
        "build/**",
        ".next/**",
        "!.next/cache/**",
        "storybook-static/**",
        "dist/**"
      ]
    },
    "check:type": {
      "dependsOn": ["^check:type"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "generate": {
      "dependsOn": ["^generate"],
      "inputs": ["$TURBO_DEFAULT$", ".env.example", ".env"],
      "outputs": ["src/**"],
      "cache": false
    },
    "storybook:dev": {
      "cache": false,
      "persistent": true
    },
    "storybook:build": {
      "outputs": ["storybook-static/**"]
    }
  }
}
