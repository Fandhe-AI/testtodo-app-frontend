{
  "name": "@repo/shared-config-lingui",
  "description": "Lingui config for the project",
  "version": "0.0.0",
  "dependencies": {},
  "devDependencies": {
    "@lingui/cli": "5.6.1",
    "@lingui/format-json": "5.6.1",
    "@repo/shared-config-typescript": "workspace:*",
    "@repo/shared-constant": "workspace:*",
